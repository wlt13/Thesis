keywords = {Boundary element method,Cohesive zone model,Fatigue crack growth modeling,Fatigue crack initiation modeling,Progressive damage,Quasibrittle materials},
number = {22–23},
pages = {3927--3944},
title = {{A cohesive zone model for fatigue crack growth in quasibrittle materials}},
url = {http://www.sciencedirect.com/science/article/pii/S0020768300002535 http://ac.els-cdn.com/S0020768300002535/1-s2.0-S0020768300002535-main.pdf?{\_}tid=bf720882-4856-11e4-8402-00000aab0f6c{\&}acdnat=1412049994{\_}56ee52160fe7cf176d702c400ccfb023},
