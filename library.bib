isbn = {0022-5096},
journal = {Journal of the Mechanics and Physics of Solids},
number = {9},
pages = {1397--1434},
title = {{Numerical simulations of fast crack growth in brittle solids}},
url = {http://www.sciencedirect.com/science/article/pii/0022509694900035 http://ac.els-cdn.com/0022509694900035/1-s2.0-0022509694900035-main.pdf?{\_}tid=58ae2392-47c5-11e4-a50c-00000aab0f01{\&}acdnat=1411987545{\_}30e98a526d16aa53937913bb604d6088},
volume = {42},
